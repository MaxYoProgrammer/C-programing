#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int n,r,i,sum=0;
    scanf("%d", &n);
    i=n;
    //Complete the code to calculate the sum of the five digits on n.
    while(i!=0){
        r=i%10;
        sum=sum+r;
        i=i/10;
    }
    printf("%d",sum);
    return 0;
}
